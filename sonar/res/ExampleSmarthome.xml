<?xml version="1.0" encoding="utf-8"?>

<organisation>
	<protocol>
		<name>OrderBanq</name>
			<role>Owner</role>
			<role>Home</role>
		<file>res/protocols/arrangeCelebration.rnw</file>
	</protocol>
	<protocol>
		<name>MakeBanq</name>
		<role>Cook</role>
		<role>Butler</role>
		<file>res/protocols/arrangeCelebration1.rnw</file>
	</protocol>
	<protocol>
		<name>MakeFood</name>
		<role>Cook</role>
		<role>Baker</role>
		<file>res/protocols/arrangeCelebration2.rnw</file>
	</protocol>
	<protocol>
		<name>MakeDrinks</name>
		<role>Butler</role>
		<role>Brewer</role>
		<file>res/protocols/arrangeCelebration3.rnw</file>
	</protocol>
	<opa>
		<name>O1</name>
		<task>
			<exec>
				<input>
					<protocol>OrderBanq</protocol>
					<role>Owner</role>
				</input>
			</exec>
			<split>
				<input>
					<protocol>OrderBanq</protocol>
					<role>Owner</role>
					<role>Home</role>
				</input>
				<output>
					<protocol>OrderBanq</protocol>
					<role>Owner</role>
				</output>
				<output>
					<protocol>OrderBanq</protocol>
					<role>Home</role>
				</output>
			</split>
			<split>
				<input>
					<protocol>MakeBanq</protocol>
					<role>Cook</role>
					<role>Butler</role>
				</input>
				<output>
					<protocol>MakeBanq</protocol>
					<role>Cook</role>
				</output>
				<output>
					<protocol>MakeBanq</protocol>
					<role>Butler</role>
				</output>
			</split>
			<refine>
				<input>
					<protocol>OrderBanq</protocol>
					<role>Home</role>
				</input>
				<output>
					<protocol>MakeBanq</protocol>
					<role>Cook</role>
					<role>Butler</role>
				</output>
			</refine>
			<deleg>
				<input>
					<protocol>MakeBanq</protocol>
					<role>Cook</role>
				</input>
				<to>O2</to>
			</deleg>
			<deleg>
				<input>
					<protocol>MakeBanq</protocol>
					<role>Butler</role>
				</input>
				<to>O3</to>
			</deleg>
		</task>
		<resource>resourceDummy1</resource>
		<communicationProtocol>protocolDummy1</communicationProtocol>
	</opa>
	<opa>
		<name>O2</name>
		<task>
			<exec>
				<input>
					<protocol>MakeBanq</protocol>
					<role>Cook</role>
				</input>
			</exec>
			<exec>
				<input>
					<protocol>MakeFood</protocol>
					<role>Cook</role>
				</input>
			</exec>
			<split>
				<input>
					<protocol>MakeFood</protocol>
					<role>Cook</role>
					<role>Baker</role>
				</input>
				<output>
					<protocol>MakeFood</protocol>
					<role>Cook</role>
				</output>
				<output>
					<protocol>MakeFood</protocol>
					<role>Baker</role>
				</output>
			</split>
			<refine>
				<input>
					<protocol>MakeBanq</protocol>
					<role>Cook</role>
				</input>
				<output>
					<protocol>MakeFood</protocol>
					<role>Cook</role>
					<role>Baker</role>
				</output>
			</refine>
			<deleg>
				<input>
					<protocol>MakeFood</protocol>
					<role>Baker</role>
				</input>
				<to>O4</to>
			</deleg>
		</task>
		<resource>resourceDummy2</resource>
		<resource>resourceDummy6</resource>
		<communicationProtocol>protocolDummy1</communicationProtocol>
	</opa>
	<opa>
		<name>O3</name>
		<task>
			<exec>
				<input>
					<protocol>MakeBanq</protocol>
					<role>Butler</role>
				</input>
			</exec>
			<exec>
				<input>
					<protocol>MakeDrinks</protocol>
					<role>Butler</role>
				</input>
			</exec>
			<split>
				<input>
					<protocol>MakeDrinks</protocol>
					<role>Butler</role>
					<role>Brewer</role>
				</input>
				<output>
					<protocol>MakeDrinks</protocol>
					<role>Butler</role>
				</output>
				<output>
					<protocol>MakeDrinks</protocol>
					<role>Brewer</role>
				</output>
			</split>
			<refine>
				<input>
					<protocol>MakeBanq</protocol>
					<role>Butler</role>
				</input>
				<output>
					<protocol>MakeDrinks</protocol>
					<role>Butler</role>
					<role>Brewer</role>
				</output>
			</refine>
			<deleg>
				<input>
					<protocol>MakeDrinks</protocol>
					<role>Brewer</role>
				</input>
				<to>O5</to>
			</deleg>
		</task>
		<resource>resourceDummy3</resource>
		<resource>resourceDummy7</resource>
		<communicationProtocol>protocolDummy2</communicationProtocol>
	</opa>
	<opa>
		<name>O4</name>
		<task>
			<exec>
				<input>
					<protocol>MakeFood</protocol>
					<role>Baker</role>
				</input>
			</exec>
		</task>
		<resource>resourceDummy4</resource>
		<communicationProtocol>protocolDummy4</communicationProtocol>
	</opa>
	<opa>
		<name>O5</name>
		<task>
			<exec>
				<input>
					<protocol>MakeDrinks</protocol>
					<role>Brewer</role>
				</input>
			</exec>
		</task>
		<resource>resourceDummy5</resource>
		<communicationProtocol>protocolDummy5</communicationProtocol>
	</opa>
</organisation>
